<?xml version="1.0"?>
<MethodDefinition>
	<Common>
		   <Description>
			Lo scopo del servizio è di annullare un evento di tipo farmaceutico precedentemente pubblicato. L’annullamento è relativo ad un cittadino ed una prescrizione specificati.
		   </Description>
		<Owner>DCSanita</Owner>
		<MethodName>FSE.annullaEventoFarmaceutico</MethodName>
		<Namespace>http://www.crs.lombardia.it/schemas/DCSanita/FSE/2009-01/annullaEventoFarmaceutico/</Namespace>
		<ValidateAlways>False</ValidateAlways>
		<RequireTransform>False</RequireTransform>
		<RequirePlugin>False</RequirePlugin>
		<InvokeViaPDL>True</InvokeViaPDL>
		<InvokeViaPDS>False</InvokeViaPDS>
		<AccessConditions>
			RUOLO="1" or RUOLO="51"  or
			RUOLO="7" or RUOLO="57"
		</AccessConditions>
		<SLA>
			<Availability>9900</Availability>
			<ResponseTime>2500</ResponseTime>
		</SLA>
	</Common>
	<CASE>
		<CitNO>
			<DIMA>
				<DFaccepted>False</DFaccepted>
				<ApplicationDocument name="eventoFarmaceutico" min="1" max="1">
					<D_opLegSign>False</D_opLegSign>
					<D_opWeakSign>False</D_opWeakSign>
					<D_citAttes>False</D_citAttes>
					<D_feSign>False</D_feSign>
					<D_crypt>False</D_crypt>
					<D_tStamp>False</D_tStamp>
				</ApplicationDocument>
			</DIMA>
		</CitNO>
		<CitSI>
			<DIMA>
				<DFaccepted>False</DFaccepted>
				<ApplicationDocument name="eventoFarmaceutico" min="1" max="1">
					<D_opLegSign>False</D_opLegSign>
					<D_opWeakSign>False</D_opWeakSign>
					<D_citAttes>False</D_citAttes>
					<D_feSign>False</D_feSign>
					<D_crypt>False</D_crypt>
					<D_tStamp>False</D_tStamp>
				</ApplicationDocument>
			</DIMA>
		</CitSI>
	</CASE>
	<DIMAresponse>
		<ApplicationDocument name="param" min="1" max="1">
			<D_feSign>False</D_feSign>
			<D_crypt>False</D_crypt>
			<D_tStamp>False</D_tStamp>
		</ApplicationDocument>
	</DIMAresponse>
</MethodDefinition>
