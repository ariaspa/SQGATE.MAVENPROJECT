<?xml version="1.0" encoding="utf-8"?>
<CanvasFSE xmlns="com.li.dc.sebc.turboFSE.view.component.*" 
	horizontalScrollPolicy="off" backgroundAlpha="1"
	xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%">
	
	<mx:Script>
		<![CDATA[
			[Bindable] public var navigationText:String;
		]]>
	</mx:Script>
	
	<mx:Label
		truncateToFit="true"
		width="100%" 
		text="{navigationText}" 
		selectable="false" 
		fontWeight="bold"/>
</CanvasFSE>
